<div class="container container-body float-start">

    <div class="row">
        @foreach(var dynamicField in Container.DynamicFields)
        {
            <DynamicComponent Type="dynamicField.DynamicComponent" Parameters="dynamicField.Parameters" />
        }
    </div>

    <div class="row">
        @foreach(var dynamicContainer in Container.DynamicContainers)
        {
            <DynamicComponent Type="dynamicContainer.DynamicComponent" Parameters="dynamicContainer.Parameters" />
        }
    </div>

</div>