@page "/list/{config}"
@inherits ListBase
@using System

<AuthorizeView>
    <Authorized Context="list">
        @if (config == null)
        {
            <p><em>Loading...</em></p>
        }
        else
        {
            @RenderListView()
        }
    </Authorized>
    <NotAuthorized>
        <p>Please log in.</p>
    </NotAuthorized>
</AuthorizeView>